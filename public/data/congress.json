{
    "endpoints": [
        {
            "name": "Located Legislators",
            "methods": [
                {
                    "MethodName": "legislators/locate",
                    "Synopsis": "Find members of Congerss by a latitude and longitude, or by a zip code. There is no support for operators, ordering, or partial responses. At-large districts, which encompass an entire state, are assigned a district number of 0.",
                    "HTTPMethod": "GET",
                    "URI": "/legislators/locate.:format",
                    "RequiresOAuth": "N",
                    "parameters": [
                        {
                            "Name": "format",
                            "Required": "Y",
                            "Default": "json",
                            "Description": "Output format as JSON or XML",
                            "Type": "enumerated",
                            "EnumeratedList": ["json", "xml"]
                        },
                        {
                            "Name": "latitude",
                            "Required": "Y",
                            "Type": "lat/long",
                        },
                        {
                            "Name": "longitude",
                            "Required": "Y",
                            "Type": "lat/long",
                        },
                        {
                            "Name": "zip",
                            "Required": "N",
                            "Description": "Zip code of the district.  We recommend using lat/long instead since zip codes are much more imprecise."
                            "Type": "integer",
                        }
                    ]
                },
                {
                    "MethodName": "legislators",
