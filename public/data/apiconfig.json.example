{
    "openstates": {
        "name": "Open States",
        "protocol": "http",
        "baseURL": "openstates.org",
        "publicPath": "/api/v1/",
        "headers": {
            "User-agent": "Sunlight iodocs"
        },
        "auth": "key",
        "keyParam": "apikey"
    },

    "congress": {
        "name": "Congress",
        "protocol": "http",
        "baseURL": "congress.api.sunlightfoundation.com",
        "publicPath": "/",
        "headers": {
            "X-APIKEY": "your_key_here"
        }
    },

    "influenceexplorer": {
        "name": "Influence Explorer",
        "protocol": "http",
        "baseURL": "transparencydata.com",
        "publicPath": "/api/1.0",
        "auth": "key",
        "keyParam": "apikey"
    },

    "capitolwords": {
        "name": "Capitol Words",
        "protocol": "http",
        "baseURL": "capitolwords.org",
        "publicPath": "/api/1/",
        "booleanTrueVal": "true",
        "booleanFalseVal": "false",
        "auth": "key",
        "keyParam": "apikey"
    }
}